pkgrel=1
pkgdesc="Test program for ertixnodes"
url="https://ertixnodes.xyz"
arch="all"
options="!check"
license="custom"

build() {
	echo "Build done!"
}
package() {
	mkdir -p "$pkgdir"
}

install="$pkgname.pre-install $pkgname.pre-upgrade $pkgname.post-upgrade
	$pkgname.post-install $pkgname.pre-deinstall $pkgname.post-deinstall"