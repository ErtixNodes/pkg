depends="docker"

build() {
	echo "Building not needed."
	echo "Build done!"
}
package() {
	mkdir -p "$pkgdir"
}

install="$pkgname.pre-install $pkgname.pre-upgrade $pkgname.post-upgrade $pkgname.post-install $pkgname.pre-deinstall $pkgname.post-deinstall"
